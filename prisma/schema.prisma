generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Instance {
  id            String   @id @default(cuid())
  name          String
  owner         String?
  phone         String?
  webhook       String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
